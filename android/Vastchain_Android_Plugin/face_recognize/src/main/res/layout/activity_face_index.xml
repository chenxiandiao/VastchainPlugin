<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_go_face_eye_mouth"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="打开人脸识别页面(眨眼，张嘴)" />

    <TextView
        android:id="@+id/tv_go_face_eye"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="打开人脸识别页面(眨眼)" />

    <TextView
        android:id="@+id/tv_go_face_mouth"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="打开人脸识别页面(张嘴)" />

    <TextView
        android:id="@+id/tv_go_face"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="打开人脸识别页面(无活体检测)" />
</LinearLayout>